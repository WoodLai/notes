{
    "plugins": [
        "-sharing",
        "-livereload",
        "-lunr",
        "-search",
        "search-pro",
        "back-to-top-button",
        "chapter-fold",
        "splitter",
        "pageview-count",
        "theme-comscore",
        "summary",
        "github",
        "code",
        "todo",
        "flexible-alerts",
        "mermaid-gb3",
        "tbfed-pagefooter",
        "katex"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/woodlai/note.git"
        },
        "code": {
            "copyButtons": true
        },
        "todo": {
            "label": "TODO",
            "pattern": "^\\s*TODO",
            "file": "Todo/Todo.md"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright woodlai",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}